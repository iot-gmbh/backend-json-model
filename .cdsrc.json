{
  "[development]": {
    "folders": {
      "app": "app"
    }
  },
  "[test]": {
    "folders": {
      "app": "dist"
    }
  },
  "[production]": {
    "folders": {
      "app": "dist"
    }
  },
  "requires": {
    "auth": {
      "restrict_all_services": false,
      "impl": "cds-msal-auth",
      "[development]": {
        "kind": "jwt-auth"
      },
      "[test]": {
        "kind": "jwt-auth"
      },
      "[production]": {
        "kind": "jwt-auth"
      }
    },
    "db": {
      "kind": "postgres"
    },
    "postgres": {
      "dialect": "plain",
      "impl": "cds-pg",
      "model": [
        "srv"
      ],
      "credentials": {
        "[production]": {
          "ssl": {
            "rejectUnauthorized": false
          }
        }
      }
    },
    "uaa": {
      "kind": "xsuaa"
    }
  }
}