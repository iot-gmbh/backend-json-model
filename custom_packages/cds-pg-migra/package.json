{
  "name": "cds-pg-migra",
  "version": "0.0.0-semantic-release",
  "description": "Migrate Postgres-databases for SAPCAP CDS-projects.",
  "author": "Benedikt HÃ¶lker <benedikt.hoelker@proton.me>",
  "license": "Apache-2.0",
  "type": "module",
  "engines": {
    "node": ">= 16.11 <17"
  },
  "exports": "./build/src/index.js",
  "bin": {
    "cds-pg-migra": "./build/src/index.js"
  },
  "scripts": {
    "preinstall": "pip install migra",
    "start": "nodemon --watch  \"src/**\" --ext  \"ts,json \" --exec \"npx ts-node\" src/index.ts",
    "test": "cd test/bookshop && bash ./set-env.sh local && npx ts-node --esm ../../src/index.ts",
    "test:heroku": "cd test/bookshop && bash ./set-env.sh heroku && npx ts-node --esm ../../src/index.ts",
    "create": "npm run build && npm i -g",
    "clean": "rimraf coverage build tmp",
    "docker:compose": "docker-compose -f docker-compose.yaml up -d",
    "prebuild": "npm run lint",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test:auto": "jest --coverage",
    "test:watch": "jest --watch",
    "prettier": "prettier --config .prettierrc --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sap/cds": "^5.9.8",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^9.4.0",
    "figlet": "^1.5.2",
    "fs": "^0.0.1-security",
    "globby": "^13.1.2",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "python-shell": "^3.0.1",
    "log-process-errors": "^9.1.0",
    "tslib": "~2.4",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jest": "~27.5",
    "@types/node": "^16.11.45",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "~5.26",
    "@typescript-eslint/parser": "~5.26",
    "cds-pg": "^0.1.29",
    "eslint": "~8.16",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~26.2",
    "husky": "^8.0.1",
    "jest": "~28.1",
    "nodemon": "^2.0.19",
    "prettier": "~2.6",
    "rimraf": "~3.0",
    "ts-jest": "~28.0",
    "ts-node": "^10.9.1",
    "tsutils": "~3.21",
    "typescript": "~4.7"
  },
  "volta": {
    "node": "16.13.0"
  }
}