{
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true,
    "protractor": true,
    "jquery": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier"],
  "globals": {
    "sap": "readonly",
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-underscore-dangle": 0,
    "func-names": "off"
  }
}
