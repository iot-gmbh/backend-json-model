CREATE VIEW undefined.workitemsservice_iotworkitems AS
 SELECT workitems_0.activateddate AS datum,
    workitems_0.completeddate AS datumbis,
    ''::text AS beginn,
    ''::text AS ende,
    ''::text AS p1,
    project_1.iotprojectid AS projekt,
    workpackage_2.iotpackageid AS teilprojekt,
    ''::text AS arbeitspaket,
    'Durchf√ºhrung'::text AS taetigkeit,
    assignedto_3.userprincipalname AS nutzer,
    'GE'::text AS einsatzort,
    workitems_0.title AS bemerkung,
    assignedto_3.manager_userprincipalname AS manageruserprincipalname
   FROM (((undefined.iot_planner_workitems workitems_0
     LEFT JOIN undefined.iot_planner_projects project_1 ON (((workitems_0.project_id)::text = (project_1.id)::text)))
     LEFT JOIN undefined.iot_planner_packages workpackage_2 ON (((workitems_0.workpackage_id)::text = (workpackage_2.id)::text)))
     LEFT JOIN undefined.iot_planner_users assignedto_3 ON (((workitems_0.assignedto_userprincipalname)::text = (assignedto_3.userprincipalname)::text)))
