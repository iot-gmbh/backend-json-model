--
-- Name: analyticsservice_workitems; Type: VIEW; Schema: undefined; Owner: -
--

CREATE VIEW undefined.analyticsservice_workitems AS
 SELECT workitems_0.id,
    assignedto_1.displayname AS assignedtoname,
    customer_2.name AS customername,
    project_3.title AS projecttitle,
    workpackage_4.title AS packagetitle,
    workitems_0.activateddate,
    workitems_0.completeddate,
    workitems_0.activateddatemonth,
    workitems_0.activateddateyear,
    round(workitems_0.duration, 2) AS duration,
    workitems_0.customer_id,
    workitems_0.project_id,
    workitems_0.workpackage_id,
    workitems_0.assignedto_userprincipalname
   FROM ((((undefined.iot_planner_workitems workitems_0
     LEFT JOIN undefined.iot_planner_users assignedto_1 ON (((workitems_0.assignedto_userprincipalname)::text = (assignedto_1.userprincipalname)::text)))
     LEFT JOIN undefined.iot_planner_customers customer_2 ON (((workitems_0.customer_id)::text = (customer_2.id)::text)))
     LEFT JOIN undefined.iot_planner_projects project_3 ON (((workitems_0.project_id)::text = (project_3.id)::text)))
     LEFT JOIN undefined.iot_planner_packages workpackage_4 ON (((workitems_0.workpackage_id)::text = (workpackage_4.id)::text)))
  WHERE (workitems_0.deleted IS NULL);


