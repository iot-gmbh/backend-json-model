{
  "_version": "1.9.0",
  "sap.cloud": {
    "public": true,
    "service": "iot.project.planner"
  },
  "sap.app": {
    "id": "iot.planner.components.singleplanningcalendar",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "ach": "ach",
    "dataSources": {
      "mainService": {
        "uri": "/v2/timetracking/",
        "type": "OData"
      }
    },
    "crossNavigation": {
      "inbounds": {
        "iot-singleplanningcalendar-inbound": {
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          },
          "semanticObject": "SinglePlanningCalendar",
          "action": "manage",
          "title": "{{appTitle}}",
          "subTitle": "{{appSubtitle}}",
          "icon": "sap-icon://calendar"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "sap-icon://task",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.66.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "iot.planner.components.singleplanningcalendar.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "rootView": {
      "viewName": "iot.planner.components.singleplanningcalendar.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "iot.planner.components.singleplanningcalendar.view",
        "controlId": "spcApp",
        "controlAggregation": "pages",
        "bypassed": {
          "target": [
            "notFound"
          ]
        },
        "async": true
      },
      "routes": [
        {
          "pattern": "calendar",
          "name": "calendar",
          "target": "calendar"
        },
        {
          "pattern": "singleEntry",
          "name": "singleEntry",
          "target": "singleEntry"
        },
        {
          "pattern": "masterDetail",
          "name": "masterDetail",
          "target": "masterDetail"
        }
      ],
      "targets": {
        "calendar": {
          "viewName": "SinglePlanningCalendar",
          "viewId": "singlePlanningCalendar",
          "viewLevel": 0,
          "title": "SinglePlanningCalendar"
        },
        "singleEntry": {
          "viewName": "SingleEntry",
          "viewId": "singleEntry",
          "viewLevel": 0,
          "title": "Single Entry"
        },
        "masterDetail": {
          "viewName": "MasterDetail",
          "viewId": "masterDetail",
          "viewLevel": 0,
          "title": "Entries"
        },
        "notFound": {
          "viewName": "NotFound",
          "viewId": "notFound"
        },
        "errorOnStartup": {
          "viewName": "ErrorOnStartup",
          "viewId": "errorOnStartup"
        }
      }
    }
  }
}