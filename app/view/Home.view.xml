<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="iot.planner.controller.Home"
    xmlns="sap.m" displayBlock="true">
    <Page showFooter="false" showHeader="false">
        <content>
            <GenericTile class="sapUiTinyMargin" header="{i18n>manageCategories}" press=".navTo($event, 'manageCategories')"/>
            <!-- <GenericTile class="sapUiTinyMargin" header="{i18n>calendar}" press=".navTo($event, 'trackViaCalendar')"/> -->
            <GenericTile class="sapUiTinyMargin" header="{i18n>calendar}" press=".navTo($event, 'trackViaCalendar', 'spc', 'calendar')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>masterDetail}" press=".navTo($event, 'trackViaCalendar', 'spc', 'masterDetail')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>fastEntry}" press=".navTo($event, 'fastEntry')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>workItems}" press=".navTo($event, 'workItems')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>IOTWorkItems}" press=".navTo($event, 'IOTWorkItems')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>IOTWorkItems} V2" press=".navTo($event, 'IOTWorkItemsV2')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>Users}" press=".navTo($event, 'Users')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>graph}" press="navToAnalytics"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>analyticsALP}" press=".navTo($event, 'analyticsALP')"/>
            <GenericTile class="sapUiTinyMargin" header="{i18n>analyticsALPv2}" press=".navTo($event, 'analyticsALPv2')"/>
        </content>
    </Page>
</mvc:View>
