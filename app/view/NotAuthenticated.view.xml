<mvc:View controllerName="iot.planner.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">

    <MessagePage showHeader="false" text="{i18n>notAuthenticatedTitle}" icon="sap-icon://document" id="notAuthPage" description="{i18n>notAuthenticatedText}">
        <customDescription>
            <Link id="link" text="{i18n>authenticate}" press="login"/>
        </customDescription>
    </MessagePage>

</mvc:View>